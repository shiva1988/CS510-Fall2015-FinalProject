#include "complex.h"

typedef struct cplane  { 
	long double xmin; 
	long double xmax; 
	long double ymin; 
	long double ymax; 
	unsigned long int xpoints; 
	unsigned long int ypoints; 
	ComplexNumber* mat;
} CPLANE;

CPLANE* CPLANE_new(long double xmin, long double xmax, long double ymin, long double ymax, unsigned long int xpoints, unsigned long int  ypoints);
